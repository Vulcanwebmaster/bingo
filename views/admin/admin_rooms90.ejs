<%- include('../header'); %>
<main>
	<div class="container">
        <div class="row">
            <div class="col-md-12">
                <%- include('admin_menu'); %>
            </div>
        </div>
    </div>
    <hr>
    <div class="container" style="min-height: 550px">
        <div class="row z-depth-5">
        	<h5><%- Bingo.title %></h5>
            <div class="col-md-12">
				
					<div class="row list lead">
					    	<div class="col-md-3">
					    		Name
					    	</div>
					    	<div class="col-md-7">
					    		Description
					    	</div>
					    	<div class="col-md-2">
					    	</div>
					   </div>
					
				<% if(Bingo.rooms.length > 0) { 
				  	 for(i=0;i<Bingo.rooms.length;i++){ %>

					   
					    <div class="row list">
					    	<div class="col-md-3">
					    		<%- Bingo.rooms[i].name %>
					    	</div>
					    	<div class="col-md-7"> 
					    		<%- Bingo.rooms[i].description %>
					    	</div>
					    	
		
					    	<div class="col-md-2">
					    		<a class=""  href="/admin/rooms90/view/<%- Bingo.rooms[i]._id %>"> 
					    		<i class="fa fa-pencil-square-o"></i> View/Edit</a>
					    	</div>
					    		
					    </div>
					

				 	<% } %> 	 	
				<% } else { %>
					<div class="row list">
				    	<div class="col-md-12">
				    		There is no data!
				    	</div>
					</div>
				<% } %>	
				
			</div>
		</div>
	</div>
	<hr>
</main>
<%- include('../footer'); %>	
