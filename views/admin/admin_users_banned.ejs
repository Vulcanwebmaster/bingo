<%- include('../header'); %>
<main>
	<div class="container">
        <div class="row">
            <div class="col-md-12">
                <%- include('admin_menu'); %>
            </div>
        </div>
    </div>
    <hr>
    <div class="container" style="min-height: 550px">
        <div class="row z-depth-5">
        	<h5><%- Bingo.title %></h5>
            <div class="col-md-12">
				
					<div class="row list lead">
					    	<div class="col-md-3">
					    		Username/Email
					    	</div>
					    	<div class="col-md-2">
					    		First Name
					    	</div>
					    	<div class="col-md-2">
					    		Last Name
					    	</div>
					        <div class="col-md-4">
					    	</div>
					   </div>
					
				<% if(Bingo.players.length > 0) { 
				  	 for(i=0;i<Bingo.players.length;i++){ %>

					   
					    <div class="row list">
					    	<div class="col-md-3">
					    		<%- Bingo.players[i].username %>
					    	</div>
					    	<div class="col-md-2"> 
					    		<%- Bingo.players[i].firstname %>
					    	</div>
					    	<div class="col-md-2"> 
					    		<%- Bingo.players[i].lastname %>
					    	</div>
					        <!-- <div class="col-md-1">
					    		<a class=""  href="/admin/users/edit/<%- Bingo.players[i]._id %>"> 
					    		<i class="fa fa-pencil-square-o"></i> </a>
					    	</div> -->
					    	<div class="col-md-2">
					    		<a class=""  href="/admin/users/view/<%- Bingo.players[i]._id %>"> 
					    		<i class="fa fa-pencil-square-o"></i> View/Edit</a>
					    	</div>
					    	<div class="col-md-1">
					    		<a class=""  href="/admin/users/delete/<%- Bingo.players[i]._id %>"> 
					    		<i class="fa fa-times"></i> Del</a>
					    	</div>
					    	<div class="col-md-2">
					    		<a class="t" href="/admin/users/unban/<%- Bingo.players[i]._id %>"> 
					    		<i class="fa fa-ban"></i> Unban</a>
					    	</div>	
					    </div>
					

				 	<% } %> 	 	
				<% } else { %>
					<div class="row list">
				    	<div class="col-md-12">
				    		There is no data!
				    	</div>
					</div>
				<% } %>	
				
			</div>
		</div>
	</div>
	<hr>
</main>
<%- include('../footer'); %>	
